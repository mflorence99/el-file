{
  "name": "elfile",
  "main": "dist/electron.js",
  "description": "Elf",
  "author": "Mark Florence <mflo999@gmail.com>",
  "version": "1.0.0",
  "license": "MIT",
  "build": {
    "appId": "elfile"
  },
  "scripts": {
    "build": "rimraf dist && ng build --aot && tsc -p tsconfig.main.json",
    "clean": "rm -rf node_modules && rm package-lock.json && npm install",
    "deb64": "electron-installer-debian --src build/elfile-linux-x64/ --arch amd64 --config installer.json",
    "live": "npm run build && electron . --ignore-gpu-blacklist",
    "package": "npm run build && rimraf build && electron-packager ./ --out=build",
    "postinstall": "electron-rebuild -f && npm rebuild sass",
    "serve": "ng serve",
    "test": "tsc -p tsconfig.main.json && DEV_MODE=1 electron . --ignore-gpu-blacklist",
    "toc": "markdown-toc -i README.md"
  },
  "dependencies": {
    "@angular/animations": "6",
    "@angular/cdk": "6",
    "@angular/common": "6",
    "@angular/compiler": "6",
    "@angular/core": "6",
    "@angular/forms": "6",
    "@angular/material": "6",
    "@angular/platform-browser": "6",
    "@angular/platform-browser-dynamic": "6",
    "@fortawesome/angular-fontawesome": "0.1.0-10",
    "@fortawesome/fontawesome-svg-core": "^1.2.0-11",
    "@fortawesome/free-brands-svg-icons": "^5.1.0-8",
    "@fortawesome/free-regular-svg-icons": "^5.1.0-8",
    "@fortawesome/free-solid-svg-icons": "^5.1.0-8",
    "@ngxs/devtools-plugin": "3",
    "@ngxs/logger-plugin": "3",
    "@ngxs/storage-plugin": "3",
    "@ngxs/store": "3",
    "angular-split": "1.0.0-rc.3",
    "angular2-uuid": "1",
    "angular4-drag-drop": "1",
    "async-es": "2",
    "core-js": "2",
    "ellib": "latest",
    "filewatcher": "3",
    "fs-extra": "^6.0.1",
    "hammerjs": "2",
    "moment": "2",
    "ngx-contextmenu": "5",
    "ngx-electron": "1",
    "node-dir": "^0.1.17",
    "numeral": "2",
    "roboto-fontface": "^0.9.0",
    "rxjs": "6",
    "rxjs-compat": "6",
    "stat-mode": "^0.2.2",
    "touch": "3",
    "trash": "4",
    "web-animations-js": "2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular/cli": "6",
    "@angular/compiler-cli": "6",
    "@angular/language-service": "6",
    "@types/node": "^10",
    "@types/stat-mode": "^0.2.0",
    "@types/touch": "3",
    "codelyzer": "4",
    "devtron": "1",
    "electron": "2",
    "electron-devtools-installer": "2",
    "electron-installer-debian": "^0.8.1",
    "electron-packager": "12",
    "electron-rebuild": "1",
    "eslint": "4",
    "markdown-toc": "1",
    "rimraf": "2",
    "ts-node": "6",
    "tslint": "5",
    "typescript": "~2.7"
  }
}
